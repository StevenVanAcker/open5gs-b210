APN-NAME				-- contains the name of the APN to deploy
01_clone_and_build.sh	-- clone repos and build docker containers. This also sets MCC and MNC
02_firewall.sh			-- setup iptables firewall
03_patch_apn_name.sh	-- change 'ims' to another APN name
04_start_stop.sh		-- start containers
05_nat_routing.sh		-- set routing so internet communication works
06_provision.sh			-- provision SIM cards
